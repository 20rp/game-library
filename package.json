  "bugs": {
    "url": "https://github.com/20rp/game-library/issues"
  },
  "homepage": "https://github.com/20rp/game-library#readme",
  "dependencies": {
    "browser-sync": "^2.29.1",
    "express": "^4.18.2",
    "jquery": "^3.7.0",
    "jsdom": "^21.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.3.1",
    "nodemon": "^2.0.22",
    "npm": "^9.6.6",
    "pug": "^3.0.2",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0"
