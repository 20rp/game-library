{
  "name": "game-library",
  "version": "0.0.1",
  "description": "A simple video game library",
  "main": "app.js",
  "scripts": {
    "build": "npm install express nodemon browser-sync pug mysql jquery datatables.net datatables.net-dt sequelize mysql2 --save",
    "dev": "nodemon app.js",
    "ui": "browser-sync start --config bs-config.js",
    "gamelibrary": "nodemon app.js && browser-sync start --config bs-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/20rp/game-library.git"
  },
  "keywords": [
    "games"
  ],
  "author": "20rp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/20rp/game-library/issues"
  },
  "homepage": "https://github.com/20rp/game-library#readme",
  "dependencies": {
    "browser-sync": "^2.27.11",
    "datatables.net": "^1.13.1",
    "datatables.net-dt": "^1.13.1",
    "express": "^4.18.2",
    "jquery": "^3.6.3",
    "jsdom": "^21.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.0.1",
    "nodemon": "^2.0.20",
    "npm": "^9.3.0",
    "pug": "^3.0.2",
    "sequelize": "^6.28.0"
  }
}
