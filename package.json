    "ui": "browser-sync start --config bs-config.js",
    "gamelibrary": "nodemon app.js && browser-sync start --config bs-config.js"
  },
  "repository": {
    "type": "module",
    "url": "git+https://github.com/20rp/game-library.git"
  },
